
    {% extends 'base.html.twig' %}

    {% block content %}
        <main>
            {% for articles in article %}
                <div>
                    <h1>{{ articles.title }}</h1>
                    <p>{{ articles.content }}</p>
                    <img src="{{ articles.image }}">
                </div>
            {% endfor %}
        </main>
    {% endblock %}
