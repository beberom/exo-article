
    {% extends 'base.html.twig' %}

    {% block content %}
        <main>
            {% for article in articles %}
                <div>
                    <h1>{{ article.title }}</h1>
                    <p>{{ article.content }}</p>
                    <img src="{{ article.image }}">
                </div>
            {% endfor %}
        </main>
    {% endblock %}
